{
  "name": "@digitalstage/react-hooks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DEBUG=router:*,teckos:* NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*.{ts,tsx}\"",
    "precommit": "npm run prettier:write",
    "lint": "eslint ./lib ./components --ext .js,.jsx,.ts,.tsx",
    "start": "next start",
    "deploy": "vercel --prod",
    "deploy:ci:test": "WHATEVER deploy --only hosting:test --token $FIREBASE_TOKEN",
    "deploy:ci:prod": "WHATEVER deploy --only hosting:prod --token $FIREBASE_TOKEN",
    "test": "jest --verbose",
    "test:watch": "yarn run test --watch",
    "test:ci": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --headless --browser chrome",
    "test:e2e": "start-server-and-test develop http://localhost:3000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:3000 cy:run",
    "test:e2e:ci:chrome": "start-server-and-test develop http://localhost:3000 cy:run:chrome"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "debug": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint ./lib ./components --ext .js,.jsx,.ts,.tsx --fix",
      "pretty-quick â€”staged"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "baseui": "9.105.0",
    "bowser": "^2.11.0",
    "formik": "^2.2.5",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "mediasoup-client": "^3.6.21",
    "next": "10.0.3",
    "next-redux-wrapper": "^6.0.2",
    "normalizr": "^3.6.1",
    "react": "17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "standardized-audio-context": "^25.1.6",
    "styletron-engine-atomic": "^1.4.6",
    "styletron-react": "^5.2.7",
    "teckos-client": "^0.1.2",
    "theme-ui": "^0.3.4",
    "validator": "^13.1.17",
    "webrtc-adapter": "^7.7.0",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.5.0",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/cypress": "^7.0.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-redux": "^7.1.11",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.1",
    "@types/theme-ui": "^0.3.7",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "cypress": "^6.0.0",
    "cypress-axe": "^0.12.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-emotion": "^11.0.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.3.0",
    "start-server-and-test": "^1.11.6",
    "typescript": "^4.1.2"
  }
}
