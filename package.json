{
  "name": "@digitalstage/react-hooks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn run prettier:base -- --list-different \"**/*.{ts,tsx}\"",
    "prettier:write": "yarn run prettier:base -- --write \"**/*.{ts,tsx}\"",
    "precommit": "yarn run prettier:write",
    "lint": "eslint ./lib ./components --ext .js,.jsx,.ts,.tsx",
    "start": "next start",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "baseui": "9.104.0",
