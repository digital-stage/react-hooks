    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "mediasoup-client": "^3.6.21",
    "next": "10.0.1",
    "next-redux-wrapper": "^6.0.2",
    "normalizr": "^3.6.1",
    "react": "17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "17.0.1",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "standardized-audio-context": "^25.1.5",
